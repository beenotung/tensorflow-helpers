      "optional": true
    }
  },
  "devDependencies": {
    "@tensorflow/tfjs-core": "^4.20.0",
    "@types/node": "^22.1.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
